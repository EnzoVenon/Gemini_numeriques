{
  "name": "gemini_numeriques",
  "source": "./src/index.html",
  "staticFiles": {
    "staticOutPath": "./data",
    "staticPath": "./data"
  },
  "jest": {
    "roots": [
      "<rootDir>/src/"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "scripts": {
    "start": "parcel",
    "build": "sed -i 's/\\.\\.\\/data/\\.\\/data/g' src/main.js && parcel build --dist-dir public && sed -i 's/\"\\//\"\\.\\//g' public/index.html && sed -i 's/\\.\\/data/\\.\\.\\/data/g' src/main.js",
    "clean": "rm -rf dist",
    "eslintTest": "npx eslint src",
    "test": "npx eslint src ; jest --coverage",
    "jest": "jest --coverage"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "assert": "^2.0.0",
    "babel-jest": "^29.5.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^5.7.1",
    "chart.js": "^4.2.1",
    "eslint": "^8.36.0",
    "eslint-plugin-jest": "^27.2.1",
    "events": "^3.3.0",
    "jest": "^29.5.0",
    "papaparse": "^5.4.1",
    "parcel": "^2.8.3",
    "parcel-reporter-static-files-copy": "^1.5.0",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "string_decoder": "^1.3.0",
    "util": "^0.12.5"
  },
  "dependencies": {
    "@turf/turf": "^6.5.0",
    "bootstrap": "^5.3.0-alpha2",
    "bootstrap-icons": "^1.10.4",
    "itowns": "^2.39.0",
    "leaflet-omnivore": "^0.3.0",
    "mapshaper": "^0.6.25",
    "shapefile": "^0.6.6",
    "shpjs": "^4.0.2 "
  }
}
